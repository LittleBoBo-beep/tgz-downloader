{
  "name": "tgz-package-downloader",
  "version": "1.0.3",
  "description": "download tarball from package-json",
  "main": "lib/downloader.js",
  "keywords": [
    "tgz", "tgz-package", "tgz-package-downloader"
  ],
  "author": "chenhaolin0124",
  "license": "ISC",
  "bin": {
    "download-tgz": "bin/download-tgz.js"
  },
  "scripts": {
    "package-puppeteer": "download-tgz package puppeteer",
    "packages-puppeteer": "download-tgz packages puppeteer 5",
    "package-lock": "download-tgz package-lock mock/package/package-lock.json",
    "package-lock-simple": "download-tgz package-lock mock/simple/package-lock.json",
    "package-lock-lq": "download-tgz package-lock mock/lq/package-lock.json",
    "package-json-lq": "download-tgz package-json mock/axios/package.json --devDependencies",
