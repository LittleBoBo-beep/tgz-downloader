  },
  "files": [
    "lib",
    "bin",
    "log"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "colors": "^1.4.0",
    "commander": "^10.0.1",
    "log4js": "^6.9.1",
    "mkdirp": "^0.5.1",
    "p-limit": "^4.0.0",
    "semver": "^7.5.1",
    "tar": "^6.1.15"
  },
  "devDependencies": {
    "tgz-checker": "^0.1.8"
  }
